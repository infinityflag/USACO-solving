#include <iostream>
#include <vector>
#include <algorithm>
#include <queue> 
//#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
queue<int> q;
int main() {
    ios_base::sync_with_stdio(0); cin.tie(0);
	//17 25 77
	//ax+by<=c
	int x,y,c;
	cin >> x >> y >> c;
	int ans =-1e9;
	for(int i=1;;i++){
		for(int j=1;;j++){
			ans =max(ans,(x*i+y*j));
			if(ans < c){
				continue;
			}
			//17 2B17 25 77bb5 77
			else{
				cout << ans;
				return 0;
			}
			


			5 3 4
		}
	}






}
